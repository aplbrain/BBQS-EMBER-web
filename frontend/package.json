  "productName": "EMBER Archive",
  "author": "Nicole Stock",
  "private": true,
  "type": "module",
  "scripts": {
    "lint": "eslint -c ./eslint.config.js \"./src*/**/*.{ts,js,cjs,mjs,vue}\"",
