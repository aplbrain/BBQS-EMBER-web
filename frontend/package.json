    "@typescript-eslint/parser": "^7.16.0",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.5.0",
    "autoprefixer": "^10.4.2",
    "axios": "^1.8.2",
    "eslint": "9",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-vue": "^10.0.0",
